{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "NetskopeHostName": {
            "type": "string",
            "metadata": {
                "description": "Enter the Netskope Host Name."
            }
        },
        "NetskopeAPIToken": {
            "type": "securestring",
            "metadata": {
                "description": "Enter the Netskope API Token."
            }
        },
        "AlertsTypes": {
            "type": "string",
            "minLength": 1,
            "metadata": {
                "description": "Enter Alert types in a comma-separated format. (Example: malware, malsite, policy)"
            }
        },
        "EventsTypes": {
            "type": "string",
            "minLength": 1,
            "metadata": {
                "description": "Enter Event types in a comma-separated format. (Example: audit, page, application)."
            }
        },
        "LogLevel": {
            "type": "string",
            "metadata": {
                "description": "Please add log level or log severity value. By default it is set to INFO"
            },
            "allowedValues": [
                "Debug",
                "Info",
                "Error",
                "Warning"
            ],
            "defaultValue": "Info"
        },
        "WorkspaceID": {
            "type": "string",
            "minLength": 1,
            "metadata": {
                "description": "Enter Workspace ID of Log Analytics workspace."
            }
        },
        "WorkspaceKey": {
            "type": "securestring",
            "minLength": 1,
            "metadata": {
                "description": "Enter Workspace Key of Log Analytics workspace."
            }
        }
    },
    "variables": {
        "WebTxMetrics":"[concat('WebTxMetric',uniqueString(resourceGroup().id))]",
        "alerts": "[split(parameters('AlertsTypes'), ',')]",
        "events": "[split(parameters('EventsTypes'), ',')]",
        "all_types": "[union(variables('alerts'),variables('events'))]",
        "function_names": {
            "dlp": {
                "name": "dlp"
            },
            "connection": {
                "name": "conn"
            },
            "ctep": {
                "name": "ctep"
            },
            "compromisedcredential": {
                "name": "cc"
            },
            "malware": {
                "name": "malware"
            },
            "malsite": {
                "name": "malsite"
            },
            "policy": {
                "name": "policy"
            },
            "remediation": {
                "name": "remed"
            },
            "quarantine": {
                "name": "quarant"
            },
            "securityassessment": {
                "name": "securit"
            },
            "uba": {
                "name": "uba"
            },
            "page": {
                "name": "page"
            },
            "application": {
                "name": "app"
            },
            "network": {
                "name": "network"
            },
            "infrastructure": {
                "name": "infra"
            },
            "audit": {
                "name": "audit"
            },
            "incident": {
                "name": "incid"
            }
        },
        "copy": [
            {
                "name": "AlertsEventsNameArray",
                "count": "[length(variables('all_types'))]",
                "input": {
                    "NetskopeToStorage": "[concat('NtoS',variables('function_names')[variables('all_types')[copyIndex('AlertsEventsNameArray', 0)]].name,uniqueString(resourceGroup().id))]",
                    "StorageToSentinel": "[concat('StoS',variables('function_names')[variables('all_types')[copyIndex('AlertsEventsNameArray', 0)]].name,uniqueString(resourceGroup().id))]"
                }
            }
        ],
        "StorageSuffix": "[environment().suffixes.storage]",
        "LogAnaltyicsUri": "[replace(environment().portal, 'https://portal', concat('https://', toLower(parameters('WorkspaceID')), '.ods.opinsights'))]"
    },
    "resources": [
        {
            "copy": {
                "name": "componentcopy",
                "count": "[length(variables('all_types'))]"
            },
            "type": "Microsoft.Insights/components",
            "apiVersion": "2015-05-01",
            "name": "[variables('AlertsEventsNameArray')[copyIndex()].NetskopeToStorage]",
            "location": "[resourceGroup().location]",
            "kind": "web",
            "properties": {
                "Application_Type": "web",
                "ApplicationId": "[variables('AlertsEventsNameArray')[copyIndex()].NetskopeToStorage]"
            }
        },
        {
            "copy": {
                "name": "storageaccountcopy",
                "count": "[length(variables('all_types'))]"
            },
            "type": "Microsoft.Storage/storageAccounts",
            "apiVersion": "2019-06-01",
            "name": "[tolower(variables('AlertsEventsNameArray')[copyIndex()].NetskopeToStorage)]",
            "location": "[resourceGroup().location]",
            "sku": {
                "name": "Standard_LRS",
                "tier": "Standard"
            },
            "kind": "StorageV2",
            "properties": {
                "networkAcls": {
                    "bypass": "AzureServices",
                    "virtualNetworkRules": [],
                    "ipRules": [],
                    "defaultAction": "Allow"
                },
                "supportsHttpsTrafficOnly": true,
                "encryption": {
                    "services": {
                        "file": {
                            "keyType": "Account",
                            "enabled": true
                        },
                        "blob": {
                            "keyType": "Account",
                            "enabled": true
                        }
                    },
                    "keySource": "Microsoft.Storage"
                }
            }
        },
        {
            "copy": {
                "name": "blobServicescopy",
                "count": "[length(variables('all_types'))]"
            },
            "type": "Microsoft.Storage/storageAccounts/blobServices",
            "apiVersion": "2019-06-01",
            "name": "[concat(variables('AlertsEventsNameArray')[copyIndex()].NetskopeToStorage, '/default')]",
            "dependsOn": [
                "storageaccountcopy"
            ],
            "sku": {
                "name": "Standard_LRS",
                "tier": "Standard"
            },
            "properties": {
                "cors": {
                    "corsRules": []
                },
                "deleteRetentionPolicy": {
                    "enabled": false
                }
            }
        },
        {
            "copy": {
                "name": "fileServicescopy",
                "count": "[length(variables('all_types'))]"
            },
            "type": "Microsoft.Storage/storageAccounts/fileServices",
            "apiVersion": "2019-06-01",
            "name": "[concat(variables('AlertsEventsNameArray')[copyIndex()].NetskopeToStorage, '/default')]",
            "dependsOn": [
                "[resourceId('Microsoft.Storage/storageAccounts', tolower(variables('AlertsEventsNameArray')[copyIndex()].NetskopeToStorage))]"
            ],
            "sku": {
                "name": "Standard_LRS",
                "tier": "Standard"
            },
            "properties": {
                "cors": {
                    "corsRules": []
